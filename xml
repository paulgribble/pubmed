<PubmedArticle>
    <MedlineCitation Owner="NLM" Status="MEDLINE">
        <PMID Version="1">15282262</PMID>
        <DateCreated>
            <Year>2004</Year>
            <Month>11</Month>
            <Day>19</Day>
        </DateCreated>
        <DateCompleted>
            <Year>2004</Year>
