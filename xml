<PubmedArticle>
    <MedlineCitation Owner="NLM" Status="MEDLINE">
        <PMID Version="1">15820701</PMID>
        <DateCreated>
            <Year>2005</Year>
            <Month>04</Month>
            <Day>11</Day>
        </DateCreated>
        <DateCompleted>
