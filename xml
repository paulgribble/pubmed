<PubmedArticle>
    <MedlineCitation Owner="NLM" Status="MEDLINE">
        <PMID Version="1">19144739</PMID>
        <DateCreated>
            <Year>2009</Year>
            <Month>02</Month>
            <Day>27</Day>
        </DateCreated>
        <DateCompleted>
