<PubmedArticle>
    <MedlineCitation Owner="NLM" Status="MEDLINE">
        <PMID Version="1">24381030</PMID>
        <DateCreated>
            <Year>2014</Year>
            <Month>03</Month>
            <Day>17</Day>
        </DateCreated>
        <DateCompleted>
            <Year>2014</Year>
