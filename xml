<PubmedArticle>
    <MedlineCitation Owner="NLM" Status="MEDLINE">
        <PMID Version="1">9644289</PMID>
        <DateCreated>
            <Year>1998</Year>
            <Month>09</Month>
            <Day>09</Day>
        </DateCreated>
        <DateCompleted>
            <Year>1998</Year>
