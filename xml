<PubmedArticle>
    <MedlineCitation Owner="NLM" Status="MEDLINE">
        <PMID Version="1">9860276</PMID>
        <DateCreated>
            <Year>1999</Year>
            <Month>03</Month>
            <Day>16</Day>
        </DateCreated>
        <DateCompleted>
            <Year>1999</Year>
