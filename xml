<PubmedArticle>
    <MedlineCitation Owner="NLM" Status="MEDLINE">
        <PMID Version="1">12612006</PMID>
        <DateCreated>
            <Year>2003</Year>
            <Month>05</Month>
            <Day>12</Day>
        </DateCreated>
        <DateCompleted>
            <Year>2003</Year>
