<PubmedArticle>
    <MedlineCitation Owner="NLM" Status="MEDLINE">
        <PMID Version="1">11772440</PMID>
        <DateCreated>
            <Year>2002</Year>
            <Month>01</Month>
            <Day>04</Day>
        </DateCreated>
        <DateCompleted>
