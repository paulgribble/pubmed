<PubmedArticle>
    <MedlineCitation Owner="NLM" Status="MEDLINE">
        <PMID Version="1">11156311</PMID>
        <DateCreated>
            <Year>2001</Year>
            <Month>01</Month>
            <Day>11</Day>
        </DateCreated>
        <DateCompleted>
            <Year>2001</Year>
