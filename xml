<PubmedArticle>
    <MedlineCitation Owner="NLM" Status="MEDLINE">
        <PMID Version="1">19580392</PMID>
        <DateCreated>
            <Year>2010</Year>
            <Month>04</Month>
            <Day>06</Day>
        </DateCreated>
        <DateCompleted>
            <Year>2010</Year>
