<PubmedArticle>
    <MedlineCitation Owner="NLM" Status="MEDLINE">
        <PMID Version="1">20631214</PMID>
        <DateCreated>
            <Year>2010</Year>
            <Month>09</Month>
            <Day>16</Day>
        </DateCreated>
        <DateCompleted>
